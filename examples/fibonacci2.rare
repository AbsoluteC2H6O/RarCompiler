/* Implementaci√≥n de la serie de fibonacci usando ULA y hacer mientras*/
indice ig 1
cantidadCiclos ig 10
termino1 ig 0
termino2 ig 1

hacer {
  printR(termino1)
  siguienteTermino ig termino1 plus termino2
  termino1 ig termino2
  termino2 ig siguienteTermino
  indice ig indice plus 1
} while (indice < cantidadCiclos) 